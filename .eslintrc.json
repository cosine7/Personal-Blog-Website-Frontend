{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/vue3-strongly-recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "ignorePatterns": [
        "vite.config.ts",
        "env.d.ts"
    ],
    "rules": {
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-alert": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": "off"
    }
}
